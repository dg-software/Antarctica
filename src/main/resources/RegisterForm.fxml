<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="rootAP" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="195.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="ui.RegisterFormController">
    <children>
        <TitledPane animated="false" layoutX="-47.0" layoutY="-30.0" text="Antarctica Registration"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                    AnchorPane.topAnchor="0.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="170.0" prefWidth="200.0">
                    <children>
                        <VBox layoutX="14.0" prefHeight="200.0" prefWidth="100.0" spacing="12.0"
                              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                              AnchorPane.topAnchor="0.0">
                            <padding>
                                <Insets bottom="15.0" left="15.0" right="15.0" top="12.0"/>
                            </padding>
                            <children>
                                <HBox prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label prefHeight="24.0" prefWidth="85.0" text="Login"/>
                                        <TextField fx:id="loginField" prefHeight="25.0" prefWidth="270.0"/>
                                    </children>
                                </HBox>
                                <HBox layoutX="25.0" layoutY="22.0" prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label prefHeight="24.0" prefWidth="85.0" text="Display name"/>
                                        <TextField fx:id="nameField" prefHeight="25.0" prefWidth="270.0"/>
                                    </children>
                                </HBox>
                                <HBox prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <Label prefHeight="24.0" prefWidth="85.0" text="Password"/>
                                        <PasswordField fx:id="passwordField" prefHeight="25.0" prefWidth="270.0"/>
                                    </children>
                                </HBox>
                                <HBox prefHeight="100.0" prefWidth="200.0">
                                    <children>
                                        <AnchorPane prefHeight="200.0" prefWidth="350.0">
                                            <children>
                                                <ButtonBar prefHeight="40.0" prefWidth="200.0"
                                                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                                           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                                    <buttons>
                                                        <Button fx:id="cancelButton" cancelButton="true"
                                                                mnemonicParsing="false" onAction="#cancelClicked"
                                                                text="Cancel"/>
                                                        <Button fx:id="registerButton" defaultButton="true"
                                                                mnemonicParsing="false" onAction="#registerClicked"
                                                                text="Register"/>
                                                    </buttons>
                                                </ButtonBar>
                                            </children>
                                        </AnchorPane>
                                    </children>
                                </HBox>
                            </children>
                        </VBox>
                    </children>
                </AnchorPane>
            </content>
        </TitledPane>
    </children>
</AnchorPane>
