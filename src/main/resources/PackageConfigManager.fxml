<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="rootAP" prefHeight="300.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="ui.packages.PackageConfigManagerController">
    <children>
        <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label fx:id="label" text="Manage configs of">
                    <font>
                        <Font size="28.0"/>
                    </font>
                </Label>
                <HBox prefHeight="200.0" prefWidth="380.0" spacing="10.0">
                    <children>
                        <ListView fx:id="configsListView" prefHeight="200.0" prefWidth="340.0"/>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
                            <children>
                                <Button fx:id="addConfigButton" mnemonicParsing="false" onAction="#onAddClicked"
                                        text="Add"/>
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                            </padding>
                        </VBox>
                    </children>
                </HBox>
                <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                        <Button fx:id="okButton" defaultButton="true" layoutX="305.0" layoutY="14.0"
                                mnemonicParsing="false" onAction="#okClicked" text="OK"/>
                    </buttons>
                </ButtonBar>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</AnchorPane>
